{
  "name": "rngraphqlweather",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "npm run pod:install",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "pod:install": "(cd ios && pod install)",
    "start": "react-native start",
    "test": "npm run test:watch",
    "test:watch": "jest -o --watch --detectOpenHandles",
    "test:coverage": "jest --coverage --verbose --logHeapUsage --maxWorkers=2",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prestorybook": "rnstl",
    "storybook:dev": "start-storybook -p 7007",
    "storybook": "adb reverse tcp:7007 tcp:7007 && npm run prestorybook && npm run storybook:dev",
    "build-storybook": "build-storybook",
    "typescript": "tsc",
    "pre-commit": "npm-run-all --parallel  test:coverage lint typescript",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "dependencies": {
    "@apollo/client": "3.5.6",
    "@react-native-async-storage/async-storage": "1.15.14",
    "@react-native-community/masked-view": "0.1.11",
    "@react-navigation/bottom-tabs": "6.0.9",
    "@react-navigation/native": "6.0.6",
    "@react-navigation/stack": "6.0.11",
    "@storybook/addon-actions": "6.4.9",
    "@storybook/addon-controls": "6.4.9",
    "@storybook/addon-links": "6.4.9",
    "@storybook/addon-ondevice-actions": "5.3.23",
    "@storybook/react-native": "5.3.25",
    "dayjs": "1.10.7",
    "eslint-config-prettier": "8.3.0",
    "graphql": "16.2.0",
    "react": "17.0.2",
    "react-native": "0.66.4",
    "react-native-dotenv": "3.3.1",
    "react-native-gesture-handler": "2.1.0",
    "react-native-reanimated": "2.3.1",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "3.10.1",
    "react-native-svg": "12.1.1",
    "react-native-switch-selector": "2.1.4",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@babel/runtime": "7.16.7",
    "@react-native-community/eslint-config": "3.0.3",
    "@storybook/addon-knobs": "6.4.0",
    "@storybook/react-native-server": "5.3.23",
    "@testing-library/jest-native": "4.0.4",
    "@testing-library/react-native": "9.0.0",
    "@types/jest": "27.5.2",
    "@types/react": "17.0.38",
    "@types/react-native": "0.66.11",
    "@types/react-native-dotenv": "0.2.0",
    "@types/react-test-renderer": "17.0.1",
    "@types/styled-components": "5.1.25",
    "@types/styled-components-react-native": "5.1.3",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "babel-jest": "27.4.5",
    "babel-loader": "8.2.3",
    "babel-plugin-module-resolver": "4.1.0",
    "eslint": "8.20.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-import-resolver-babel-module": "5.3.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-native": "4.0.0",
    "eslint-plugin-unicorn": "43.0.2",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "jest-styled-components": "7.0.8",
    "metro-react-native-babel-preset": "0.66.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "react-dom": "17.0.2",
    "react-native-storybook-loader": "2.0.4",
    "react-native-svg-transformer": "1.0.0",
    "react-test-renderer": "17.0.2",
    "ts-jest": "27.1.5",
    "typescript": "4.5.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.stories.tsx"
    }
  },
  "engines": {
    "node": ">=14.17.6",
    "npm": ">=7.24.2"
  }
}
